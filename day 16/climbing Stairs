public class ClimbingStairs {
    public static void main(String[] args) {
        int n = 5;  // Example number of stairs

        if (n <= 2) {
            System.out.println("Ways to climb " + n + " stairs: " + n);
            return;
        }

        int first = 1, second = 2;
        for (int i = 3; i <= n; i++) {
            int third = first + second;
            first = second;
            second = third;
        }

        System.out.println("Ways to climb " + n + " stairs: " + second);
    }
}
